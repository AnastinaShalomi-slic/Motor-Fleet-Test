drop SEQUENCE QUOTATION.FIRE_DH_ID_SEQ 
CREATE SEQUENCE QUOTATION.FIRE_DH_ID_SEQ 
    INCREMENT BY 1
    START WITH 1
    MINVALUE 1
    MAXVALUE 999999999999999999999999999
    CACHE 20;
